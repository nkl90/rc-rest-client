{
    "name": "nkl/rocketchat-rest-client",
    "type": "library",
    "description": "RocketChat rest client based on Guzzle Service Description",
    "keywords": [
        "nkl",
        "rocketchat-rest-client"
    ],
    "homepage": "https://github.com/nkl/rocketchat-rest-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Deriglazov",
            "email": "nikinik@inbox.ru",
            "homepage": "http://deriglazov.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-curl": "^7.4",
        "ext-json": "*",
        "guzzle/service": "^3.9",
        "knplabs/knp-paginator-bundle": "^5.4",
        "psr/http-message": "^1.0",
        "symfony/http-foundation": "^4.4",
        "symfony/options-resolver": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nkl\\RocketChatRestClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nkl\\RocketChatRestClient\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
